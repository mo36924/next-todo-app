generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator zod {
  provider   = "zod-prisma-types"
  output     = "../node_modules/@zod-prisma-types"
  coerceDate = false
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
