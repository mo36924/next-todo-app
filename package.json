{
  "name": "next-todo-app",
  "version": "0.1.0",
  "private": true,
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/vercel/next.js/tree/canary/packages/create-next-app).",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "scripts": {
    "dev": "npm-run-all clean codegen -p dev:*",
    "dev:next": "DATABASE_URL=file:${PWD}/prisma.db next dev",
    "dev:open": "open-cli http://localhost:3000",
    "build": "run-s clean codegen build:*",
    "build:next": "next build",
    "start": "DATABASE_URL=file:${TMPDIR}prisma.db run-s start:*",
    "start:prisma": "prisma migrate deploy",
    "start:next": "next start",
    "deploy": "gcloud run deploy nextjs-app-router --region asia-northeast1 --allow-unauthenticated --max-instances 1 --source .",
    "lint": "npm-run-all clean codegen --aggregate-output -p lint:* build:next",
    "lint:prettier": "prettier --check .",
    "lint:next": "next lint",
    "clean": "del .next",
    "format": "run-s format:*",
    "format:sort-package-json": "sort-package-json",
    "format:prisma": "prisma format",
    "format:next": "next lint --fix",
    "format:prettier": "prettier --write .",
    "codegen": "run-p codegen:*",
    "codegen:panda": "panda codegen",
    "codegen:prisma": "prisma generate",
    "prisma": "DATABASE_URL=file:${PWD}/prisma.db prisma"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^5.4.1",
    "@tanstack/react-query": "^4.35.7",
    "@trpc/client": "^10.38.5",
    "@trpc/next": "^10.38.5",
    "@trpc/react-query": "^10.38.5",
    "@trpc/server": "^10.38.5",
    "cookie": "^0.5.0",
    "next": "^13.5.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "superjson": "^1.13.3",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.15.3",
    "@types/cookie": "^0.5.2",
    "@types/node": "^20.8.2",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "del-cli": "^5.1.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.6.0",
    "typescript": "^5.2.2",
    "zod-prisma-types": "^2.7.9"
  }
}
